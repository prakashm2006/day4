#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    scanf("%d",&a); 
    if(a>0 && (a & (a-1))==0)
        printf("YES\n");
    else
        printf("NO\n");

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
